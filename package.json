{
  "name": "@ferry/ferry-mw-core",
  "version": "0.0.1",
  "description": "HTTP middleware core service for FerryJS",
  "main": "dist/bundle.cjs.min.js",
  "module": "dist/bundle.esm.min.js",
  "unpkg": "dist/bundle.umd.min.js",
  "devMain": "src/index.ts",
  "typings": "dist/types/index.d.ts",
  "author": "yanpan",
  "repository": {
    "type": "git",
    "url": "https://github.com/YanPanMichael/ferry-mw-core.git"
  },
  "bugs": {
    "url": "https://github.com/YanPanMichael/ferry-mw-core/issues"
  },
  "homepage": "https://github.com/YanPanMichael/ferry-mw-core",
  "keywords": [
    "fetch",
    "request",
    "adapter",
    "middleware",
    "axios",
    "http"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env BABEL_ENV=test NODE_ENV=testing jest --coverage",
    "test:watch": "jest --watch",
    "build": "cross-env NODE_ENV=production struk build --source=ts",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --fix",
    "format": "prettier --write .",
    "release": "qkrelease",
    "changelog:help": "conventional-changelog --help",
    "changelog": "conventional-changelog -p simple -i CHANGELOG.md -s -r 0",
    "docs": "npx docsify-cli serve ./docs"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^16.2.4",
    "@types/jest": "^27.0.1",
    "@types/node": "~16.11",
    "@typescript-eslint/eslint-plugin": "~5.14",
    "@typescript-eslint/parser": "~5.14",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "5.2.0",
    "eslint": "~8.11",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "^26.1.5",
    "husky": "^4.3.8",
    "jest": "~27.5",
    "qk-release": "^1.1.2",
    "struk": "^1.0.4",
    "ts-jest": "^27.1.4",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "axios": ">= 0.19.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "changelogConfig": {
    "authorName": true,
    "authorEmail": true
  }
}
